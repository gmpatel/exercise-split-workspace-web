<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
 <configuration>

   <configSections>
     <section name="WorkerConfigurationFake" type="Dataract.e5.Fake.WM.RequestManager.WorkerConfigurationSection, Dataract.e5.Fake.WM.RequestManager"/>
     <section name="WorkerConfigurationRACTi" type="Dataract.e5.RACTi.WM.RequestManager.WorkerConfigurationSection, Dataract.e5.RACTi.WM.RequestManager"/>
   </configSections>

   <!--
   <WorkerConfigurationFake
      E5SiteRootUrlFake="http://ractpoc-e5.cloudapp.net:8051"
      AnotherSiteRootUrlFake="http://ractpoc-e5.cloudapp.net:8052"
   />
   -->

   <WorkerConfigurationFake
      E5SiteRootUrlFake="http://gm-pc:90"
      AnotherSiteRootUrlFake="http://gm-pc:89"
   />

   <WorkerConfigurationRACTi
     E5ContentDbConnectionString="Data Source=RACTPOC-SQL; Initial Catalog=e5_Content_RACTi; Integrated Security=True"
     E5FindPolicyPageUrlTemplate="http://ractpoc-e5.cloudapp.net/sites/poc/e5/Find%20Policy.aspx?PolicyId=$#policy_number#$"
     E5WorkUrlTemplate="http://ractpoc-e5.cloudapp.net/sites/poc/_layouts/15/e5/WorkProcess.aspx?id=$#work_id#$"
     PurePolicyUrlTemplate="http://ractpoc-e5.cloudapp.net:8052/DummyPolicy/Index?policynumber=$#policy_number#$"
     PureClaimUrlTemplate="http://ractpoc-e5.cloudapp.net:8052/DummyClaim/Index?claimnumber=$#claim_number#$"
  />
   
  <appSettings>
    <!-- BEGIN : IRequestManager Implementation Settings -->
    <add key="Dataract.e5.WM.Interface.IRequestManager (Not In Use)" value="Dataract.e5.Fake.WM.RequestManager.Worker, Dataract.e5.Fake.WM.RequestManager, Version=1.0.0.0, Culture=neutral"/>
    <add key="Dataract.e5.WM.Interface.IRequestManager" value="Dataract.e5.RACTi.WM.RequestManager.Worker, Dataract.e5.RACTi.WM.RequestManager, Version=1.0.0.0, Culture=neutral"/>
    <!-- END   : IRequestManager Implementation Settings -->
    
    <!-- BEGIN : Custom Configuration Settings -->
    <add key="Workspace1HomeUrl" value="http://gm-pc:90"/>
    <add key="Workspace2HomeUrl" value="http://gm-pc:89"/>
    <add key="WorkspaceManagerTitle" value="RACTi - Policy and Claims Administration"/>
    <!-- END   : Custom Configuration Settings -->
    
    <!-- BEGIN : System Configuration Settings Default From MVC 4 Project -->
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- END   : System Configuration Settings Default From MVC 4 Project -->
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <pages>
      <namespaces>
        <add namespace="System.Web.Helpers" />
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Routing" />
        <add namespace="System.Web.WebPages" />
      </namespaces>
    </pages>
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="true" />
    <handlers>
      <remove name="ExtensionlessUrlHandler-ISAPI-4.0_32bit" />
      <remove name="ExtensionlessUrlHandler-ISAPI-4.0_64bit" />
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <add name="ExtensionlessUrlHandler-ISAPI-4.0_32bit" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework\v4.0.30319\aspnet_isapi.dll" preCondition="classicMode,runtimeVersionv4.0,bitness32" responseBufferLimit="0" />
      <add name="ExtensionlessUrlHandler-ISAPI-4.0_64bit" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework64\v4.0.30319\aspnet_isapi.dll" preCondition="classicMode,runtimeVersionv4.0,bitness64" responseBufferLimit="0" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
</configuration>